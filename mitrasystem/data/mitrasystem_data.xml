<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">
        <!-- Sequences -->
        <record id="seq_mitra_project" model="ir.sequence">
            <field name="name">Mitra Project</field>
            <field name="code">mitra.project</field>
            <field name="prefix">PRJ/%(year)s/</field>
            <field name="padding">4</field>
            <field name="company_id" eval="False"/>
        </record>

        <record id="seq_mitra_handover" model="ir.sequence">
            <field name="name">Mitra Handover</field>
            <field name="code">mitra.handover</field>
            <field name="prefix">HO/%(year)s/</field>
            <field name="padding">4</field>
            <field name="company_id" eval="False"/>
        </record>

        <record id="seq_mitra_complaint" model="ir.sequence">
            <field name="name">Mitra Complaint</field>
            <field name="code">mitra.complaint</field>
            <field name="prefix">C/%(year)s/</field>
            <field name="padding">4</field>
            <field name="company_id" eval="False"/>
        </record>
    </data>
</odoo>